    "axios": "^1.11.0",
    "body-parser": "^2.2.0",
    "bootstrap": "^5.3.3",
    "browser-image-compression": "^2.0.2",
    "cloudinary": "^2.7.0",
    "concurrently": "^9.2.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "glightbox": "^3.3.1",
    "i18next": "^25.5.2",
    "i18next-browser-languagedetector": "^8.2.0",
    "jarallax": "^2.2.1",
    "mongodb": "^6.19.0",
